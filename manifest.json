{
  "manifest_version": 2,
  "name": "Persian New Tab",
  "description": "The free persian calendar from parsnevis.ir",
  "author": "Mostafa Shoa \u003Cmostafa.shoa@gmail.com>",
  "version": "0.0.1",
  "icons": {
    "128": "assets/icons/128.png",
    "64": "assets/icons/64.png",
    "32": "assets/icons/32.png",
    "48": "assets/icons/48.png",
    "16": "assets/icons/16.png"
  },

  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "permissions": [
    "storage", "history",
    "https://calendar.parsnevis.ir/"
  ]
}

